{
  "name": "mysql-slim-nodejs",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Jeremi Stadler",
  "main": "dist/index.js",
  "module": "dist/mysql3.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test",
    "bench": "node -r esbuild-register benchmark/bench.ts",
    "testOld": "node -r esbuild-register test/regressions/test-#433.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/mysql3.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/mysql3.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.0",
    "@tsconfig/recommended": "^1.0.1",
    "dts-cli": "^1.5.2",
    "esbuild": "^0.15.5",
    "esbuild-register": "^3.3.3",
    "husky": "^8.0.1",
    "size-limit": "^8.0.0",
    "@types/node": "^18.7.16",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}